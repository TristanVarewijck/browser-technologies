<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title></title>
  </head>
  <body>
    <h1>Your photo's</h1>
    <!-- <img src="" alt="" /> -->
    <% if(items.length === 0){ %>
    <h1>There is no content here!</h1>
    <% } else{ %> <% items.forEach(function(item){ %>
    <img src="images/<%=item.image%>" alt="<%=item.image%>" />
    <h1><%=item.title%></h1>
    <p><%=item.location%></p>
    <p><%=item.description%></p>
    <% }); %> <% } %>
  </body>
</html>
